{
  "name": "olympus-ui",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/olympus-ui/olympus-ui.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "build:watch": "stencil build --docs --watch",
    "start": "npm run storybook",
    "test": "stencil test --spec --e2e",
    "test:watch": "stencil test --spec --e2e --watch",
    "generate": "stencil generate",
    "dev": "npm-run-all --parallel build:watch storybook",
    "storybook": "start-storybook -p 9002 -c .storybook -s dist",
    "build-storybook": "build-storybook -c ./.storybook -o ./dist"
  },
  "dependencies": {
    "@stencil/core": "^2.12.1",
    "@stencil/react-output-target": "^0.2.0",
    "autoprefixer": "^10.4.2",
    "nanoid": "^3.2.0",
    "tailwindcss": "^3.0.15"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@stencil/postcss": "^2.1.0",
    "@stencil/sass": "^1.5.2",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/html": "^6.4.9",
    "@testing-library/jest-dom": "^5.16.1",
    "@types/jest": "^27.4.0",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^6.4.1",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.7",
    "puppeteer": "^10.0.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "license": "MIT"
}
